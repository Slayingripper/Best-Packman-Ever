<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="core.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <BorderPane prefHeight="263.0" prefWidth="322.0">
         <left>
            <Button fx:id="startbtn" mnemonicParsing="false" onAction="#startGame" prefHeight="25.0" prefWidth="145.0" text="START" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Button fx:id="setupbtn" mnemonicParsing="false" onAction="#Setup" prefHeight="25.0" prefWidth="129.0" text="SETUP" BorderPane.alignment="CENTER" />
         </right>
         <top>
            <ImageView fitHeight="131.0" fitWidth="222.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <image>
                  <Image url="@../icon/pacman.png" />
               </image>
            </ImageView>
         </top>
      </BorderPane>
   </children>
</GridPane>
